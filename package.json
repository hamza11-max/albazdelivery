{
  "name": "albazdelivery-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0 <24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "tsc --noEmit",
    "clean": "turbo run clean",
    "vercel-build": "node -e \"const fs=require('fs');['.next','app/customer/.next'].forEach(p=>{try{fs.rmSync(p,{recursive:true,force:true})}catch{}});\" && prisma generate --schema=./prisma/schema.prisma && next build --webpack",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma || exit 0",
    "prepare": "prisma generate --schema=./prisma/schema.prisma || exit 0",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:generate": "cross-env NODE_ENV=development prisma generate --schema=./prisma/schema.prisma",
    "db:push": "cross-env NODE_ENV=development prisma db push",
    "db:migrate": "cross-env NODE_ENV=development prisma migrate dev",
    "db:studio": "node scripts/start-prisma-studio.js",
    "db:seed": "cross-env NODE_ENV=development node -r @swc/register prisma/seed.ts",
    "db:create-admin": "cross-env NODE_ENV=development tsx scripts/create-admin-user.ts",
    "test": "cross-env NODE_ENV=test jest --config jest.config.ts",
    "test:watch": "cross-env NODE_ENV=test jest --config jest.config.ts --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.ts --coverage",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.config.ts --testMatch \"**/__tests__/**/*.test.ts?(x)\"",
    "test:e2e": "playwright test tests-e2e/",
    "test:e2e:ui": "playwright test tests-e2e/ --ui",
    "test:e2e:debug": "playwright test tests-e2e/ --debug",
    "test:ci": "cross-env NODE_ENV=test jest --ci --config jest.config.ts --coverage --maxWorkers=2 && playwright test tests-e2e/"
  },
  "dependencies": {
    "@albaz/ui": "file:./packages/ui",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-aspect-ratio": "latest",
